{
  "name": "Tablist",
  "version": "1.0",
  "author": "Mufasa A. (mufasa159)",
  "manifest_version": 3,
  "description": "List all opened tab URLs and copy them to clipboard",
  "action": {
    "default_popup": "index.html",
    "default_title": "Tablist"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/100.png",
    "24": "assets/100.png",
    "48": "assets/100.png",
    "128": "assets/100.png"
  },
  "permissions": [
    "tabs"
  ]
}