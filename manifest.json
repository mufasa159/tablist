{
  "name": "Tablist",
  "version": "2.0",
  "author": "Mufasa A. (mufasa159)",
  "manifest_version": 3,
  "description": "List and copy all opened tab URLs, or open multiple URLs at once",
  "action": {
    "default_popup": "index.html",
    "default_title": "Tablist"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/100.png",
    "24": "assets/100.png",
    "48": "assets/100.png",
    "128": "assets/100.png"
  },
  "permissions": [
    "tabs",
    "search"
  ]
}